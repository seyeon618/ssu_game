        </ui:VisualElement>
        <ui:VisualElement name="NextButtonContainer" style="background-color: rgba(0, 0, 0, 0); -unity-text-align: upper-right; flex-direction: row; justify-content: flex-end; width: 90%; margin-right: auto; margin-left: auto; height: 100px; align-items: flex-end;">
            <ui:VisualElement name="NextButton" style="background-image: resource(&apos;UI/arrow&apos;); width: 68px; height: 58px;" />
