<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:VisualElement name="MainContainer" style="background-image: resource(&apos;UI/background&apos;); width: 100%; height: 100%; justify-content: center;">
        <ui:Label text="What&apos;s your Name?" name="Title" style="-unity-font: resource(&apos;CooperFiveOpti-Black&apos;); -unity-font-style: bold; font-size: 60px; color: rgb(255, 255, 255); -unity-text-align: middle-center; margin-top: 20px; margin-bottom: 20px; -unity-font-definition: initial; letter-spacing: -10px;" />
        <ui:VisualElement name="InputContainer" style="width: 70%; background-color: rgba(255, 255, 255, 0.9); padding: 20px; border-radius: 10px; align-items: center; justify-content: center; flex-direction: column; margin-right: auto; margin-left: auto;">
            <ui:Label text="This Nickname is disposable. It is used to record your scores." name="Subtitle" style="-unity-font: resource(&apos;NanumGothicBold&apos;); font-size: 18px; color: rgb(150, 150, 150); -unity-text-align: middle-center; margin-bottom: 10px; -unity-font-definition: initial;" />
